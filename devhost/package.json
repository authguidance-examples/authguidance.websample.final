{
  "name": "devhost",
  "version": "1.0.0",
  "description": "Developer PC hosting for a Content Delivery Network and a Web Reverse Proxy",
  "main": "src/app.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "ts-node --files src/app.ts",
    "localweb": "cross-env DEV_CONFIG=localweb nodemon -e ts --exec \"npm run build\"",
    "localapi": "cross-env DEV_CONFIG=localapi nodemon -e ts --exec \"npm run build\""
  },
  "keywords": [],
  "author": "Gary Archer",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.0",
    "cookie-encrypter": "^1.0.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "tunnel-agent": "^0.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^14.14.10",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.3",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  }
}
