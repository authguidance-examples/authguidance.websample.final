{
  "name": "devhost",
  "version": "1.0.0",
  "description": "Hosts web content delivery and an OAuth Utility API on a developer PC",
  "main": "app.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "ts-node --files src/app.ts",
    "start": "nodemon -e ts --exec \"npm run build\"",
    "package": "ts-node src/customPackager.ts",
    "deploy": "aws s3 cp .package s3://web.authguidance-examples.com --recursive",
    "clearcache": "aws cloudfront create-invalidation --distribution-id=E1KK6YUBAK08ZY --paths \"/*\""
  },
  "keywords": [],
  "author": "Gary Archer",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "tunnel-agent": "^0.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^12.11.7",
    "core-js": "^3.3.5",
    "nodemon": "^2.0.3",
    "ts-node": "^8.4.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  }
}
