{
  "name": "finalspa",
  "version": "3.0.0",
  "description": "The Final SPA, which implements OpenID Connect using an API driven Back End for Front End",
  "main": "index.tsx",
  "scripts": {
    "lint": "npx eslint . --ext .ts,.tsx",
    "start": "npm run clean && npm run copy && webpack --config webpack/webpack.config.dev.js --watch",
    "buildRelease": "npm run clean && npm run copy && webpack --config webpack/webpack.config.prod.js",
    "copy": "cp spa.config.json index.html *.css favicon.ico dist",
    "clean": "rm -rf dist && mkdir dist"
  },
  "keywords": [],
  "author": "Gary Archer",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "guid-typescript": "^1.0.9",
    "js-event-bus": "^1.1.1",
    "moment": "^2.29.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-modal": "^3.14.4",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.16.0",
    "eslint-plugin-react": "^7.30.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
