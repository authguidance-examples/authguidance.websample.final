{
  "name": "finalspa",
  "version": "3.0.0",
  "description": "The Final SPA, which implements OpenID Connect using an API driven Back End for Front End",
  "main": "index.tsx",
  "keywords": [],
  "author": "Gary Archer",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "npx eslint . --ext .ts,.tsx",
    "start": "npm run webpackWatch",
    "build": "bash ./build.sh",
    "buildRelease": "bash ./build.sh RELEASE",
    "webpackDebug": "bash -c \"NODE_OPTIONS='--loader ts-node/esm --no-warnings' webpack --config build/webpack.config.dev.ts\"",
    "webpackWatch": "bash -c \"NODE_OPTIONS='--loader ts-node/esm --no-warnings' webpack --config build/webpack.config.dev.ts --watch\"",
    "webpackRelease": "bash -c \"NODE_OPTIONS='--loader ts-node/esm --no-warnings' webpack --config build/webpack.config.prod.ts\"",
    "purgecss": "purgecss -c purgecss.config.cjs -o dist"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "guid-typescript": "^1.0.9",
    "js-event-bus": "^1.1.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-modal": "^3.13.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "purgecss": "^5.0.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
